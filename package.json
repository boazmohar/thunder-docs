{
  "name": "thunder-docs",
  "version": "1.0.0",
  "description": "documentation for thunder and related modules",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js",
    "sync": "rm -rf ~/.ecosystem-docs && node build.js",
    "convert": "rm -rf tutorials/build && node convert.js",
    "build": "npm run sync && npm run convert && browserify index.js -o site/bundle.js",
    "deploy": "npm run build && surge site docs.thunder-project.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thunder-project/thunder-docs.git"
  },
  "browserify": {
    "transform": [
      "brfs",
      "folderify"
    ]
  },
  "keywords": [
    "thunder",
    "image",
    "time-series",
    "documentation"
  ],
  "author": "freeman-lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thunder-project/thunder-docs/issues"
  },
  "homepage": "https://github.com/thunder-project/thunder-docs#readme",
  "dependencies": {
    "embed-images": "^1.0.0",
    "folderify": "^1.2.1",
    "include-folder": "^1.0.0",
    "minidocs": "^1.1.0",
    "myopts": "^1.1.0"
  }
}
